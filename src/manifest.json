{
  "name": "Tab Group",
  "version": "1.0",
  "description": "Group several tabs within only one tab",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
 